import java.util.*;
public class hourly implements Worker{
    int price, hours, age;
    String fio;
    hourly(String fio, int age, int price, int hours)
    {
        this.fio = fio;
        this.age = age;
        this.price = price;
        this.hours = hours;
    }
    public double salary(){
        return price*hours;
    }
    @Override
    public void print(){
        System.out.println("fio: = "+fio+" age: "+age+" total outcome:"+salary());
    }
}
