import java.util.Scanner;

public class percents implements Worker{
    int price, percent, age;
    String fio;
    percents(String fio, int age, int price, int percent)

    {
        this.fio = fio;
        this.age = age;
        this.price = price;
        this.percent = percent;
    }
    public double salary(){
        return price*percent*0.01;
    }
    @Override
    public void print(){
        System.out.println("fio: = "+fio+" age: "+age+" total outcome:"+salary());
    }
}
